<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/dialog_general_1_btn"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:gravity="center"
        android:text="反馈"
        android:textColor="@color/color101010"
        android:textSize="16sp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/line_05"
        android:background="@color/line_eaeaea" />

    <TextView
        android:id="@+id/dialog_general_2_btn"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:gravity="center"
        android:text="举报"
        android:textColor="@color/color101010"
        android:textSize="16sp" />


    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="@color/line_F4F4F4" />

    <TextView
        android:id="@+id/dialog_general_cancel"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:gravity="center"
        android:text="取消"
        android:textColor="@color/color101010"
        android:textSize="16sp" />

</LinearLayout>